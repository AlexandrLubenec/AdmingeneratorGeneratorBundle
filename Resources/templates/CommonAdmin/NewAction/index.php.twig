{% block index_use %}
use Symfony\Component\HttpKernel\Exception\NotFoundHttpException;
use {{ namespace_prefix }}\{{ bundle_name }}\Form\Type\NewType;
{% endblock %}
{% block index %}

    public function indexAction()
    {
        ${{ builder.ModelClass }} = $this->getNewObject();
        
        $form = $this->createForm(new NewType(), ${{ builder.ModelClass }});
        
        return $this->render('{{ namespace_prefix }}{{ bundle_name }}:New:index.html.twig', array(
            "{{ builder.ModelClass }}" => ${{ builder.ModelClass }},
            "form" => $form->createView(),
        ));
    }
{% endblock %}